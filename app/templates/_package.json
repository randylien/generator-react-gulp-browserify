{
  "name": "<%= _.slugify(projectName) %>",
  "version": "0.0.0",
  "dependencies": {},
  "devDependencies": {
    "del": "~0.1.3",
    "gulp": "~3.8.8",
    "gulp-autoprefixer": "~1.0.1",
    "gulp-bower": "0.0.6",
    "gulp-cache": "~0.2.4",<% if (includeCoffeeScript) { %>
    "coffeeify": "latest",<% } %>
    "gulp-imagemin": "latest",<% if (includeJade) { %>
    "gulp-jade": "~0.8.0",<% } %><% if (includeJest) { %>
    "gulp-jest": "~0.2.2",<% } %>
    "gulp-jshint": "~1.8.5",
    "gulp-load-plugins": "~0.7.0",<% if (includeSass) { %>
    "gulp-ruby-sass": "~0.7.1",<% } %>
    "gulp-size": "~1.1.0",
    "gulp-useref": "~0.4.4",
    "gulp-util": "~3.0.1",
    "gulp-webserver": "latest",<% if (includeJest) { %>
    "jest": "latest",<% } %>
    "react": "latest",
    "reactify": "latest",
    "browserify-shim": "^3.8.0",
    "vinyl-source-stream": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "browserify": {
      "transform": [
          "browserify-shim",<% if (includeCoffeeScript) { %>
          "coffeeify",<% } %>
          [
            "reactify",
            {
                "es6": true
            }
          ]
      ]
  },
  "browser": {
      "jquery": "./app/bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
      "jquery": "$"
  }
}
